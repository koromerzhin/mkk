{% extends "MkkSiteBundle:Templates:admin.html.twig" %}
{% block body_content %}
	{{ form_start(form) }}
	<table class="table table-striped table-bordered table-condensed CollectionRow">
		<tbody>
			<tr>
				<td colspan=2>
					{{ form_row(form.type) }}
				</td>
			</tr>
			<tr>
				<td colspan=2>
					{{ form_row(form.info) }}
				</td>
			</tr>
			<tr>
				<td colspan=2>
					{{ form_row(form.pays) }}
				</td>
			</tr>
			<tr>
				<td>
					{{ form_row(form.cp) }}
				</td>
				<td>
					{{ form_row(form.ville) }}
				</td>
			</tr>
			<tr>
				<td>
					{{ form_row(form.gps) }}
				</td>
				<td class="text-center"></td>
			</tr>
		</tbody>
	</table>
	{{ form_end(form) }}
{% endblock %}
